                                 	LDI R16, @0		; X
                                 	MOV R10, R16	
                                 	LDI R16, @1		; Y
                                 	MOV R11, R16	
                                 	LDI R16, @2		; On/Off
                                 	MOV R12, R16	
                                 	LDI R16, @3		; Width
                                 	MOV R13, R16
                                 	RCALL screen_set_hline
                                 .endmacro
                                 
                                 .macro draw_point
                                 	LDI R16, @0		; X
                                 	MOV R10, R16	
                                 	LDI R16, @1		; Y
                                 	MOV R11, R16	
                                 	LDI R16, @2		; On/Off
                                 	MOV R12, R16	
                                 	RCALL screen_set_point
                                 .endmacro
                                 
                                 .macro draw_battleship
                                 	draw_point 
                                 
