                                 	MOV R11, R16	
                                 	LDI R16, @2		; On/Off
                                 	MOV R12, R16	
                                 	LDI R16, @3		; Width
                                 	MOV R13, R16	
                                 	LDI R16, @4		; Height
                                 	MOV R14, R16	
                                 	RCALL screen_set_rect
